<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33c52b" />
            <gradient  android:angle="-90"  android:startColor="#22a105" android:endColor="#08d672"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33c52b" />
            <solid  android:color="#33c52b"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33c52b" />
            <gradient  android:angle="-90"  android:startColor="#2bc569" android:endColor="#33c52b" />
        </shape>
    </item>
</selector>